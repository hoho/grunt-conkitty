ul_list items
    div[data-list=999]
        PAYLOAD
    ul
        CALL list (items)

ol_list items arg1
    h3
        "arg1 + 1 === "
        (arg1 + 1)
    ol
        CALL list (items)

list items
    EACH (items)
        li.item
            @num CURRENT
            "item: "
            strong
                CURRENT
                "!"

sep
    section.horizontal-separator
        "sep-sep-sep"
